NO_AUTO_COMPILING = yes
SUBDIRS := single-bid-no-announce

include $(this_srcdir)common.mk

all-recurse-single-bid-no-announce test-recurse-single-bid-no-announce: \
	hands_features

hands_all: $(SIM)
	$(SIM) -p- -m -C <($(NUMS_CMD) | sed 's/$$/ /') | \
		cut -d , -f 1 | sed 's/ $$//' | paste <($(NUMS_CMD)) - | \
		tr ' \t' '_ ' > $@

hands_features: hands_all $(FEAT)
	$(FEAT) $< > $@

